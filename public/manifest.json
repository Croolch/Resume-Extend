{
    "manifest_version": 3,
    "name": "Resume",
    "description": "我要海投简历",
    "version": "0.0.1",
    "permissions": [
        "activeTab"
    ],
    "background": {
        "service_worker": "/background.js"
    },
    "action": {
        "default_popup": "popup.html"
    },
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "/content.js"
            ]
        }
    ],
    "icons": {
    }
}