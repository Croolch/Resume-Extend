{
  "manifest_version": 3,
  "name": "Resume",
  "description": "我要海投简历",
  "version": "0.0.1",
  "background": {
    "service_worker": "/background.js"
  },
  "action": {
    
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "/content.js"
      ]
    }
  ],
  "permissions": [
    "activeTab",
    "scripting"
  ]
}